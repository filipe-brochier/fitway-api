{
  "name": "fitway-api",
  "version": "1.0.0",
  "description": "**FitWay** é uma simples e poderosa API RESTful desenvolvida com Node.js, com o objetivo de consolidar aprendizados sobre autenticação, regras de negócio, validação, segurança e estruturação de projetos reais. Inspirada no modelo Gympass, a aplicação permite check-ins em academias, gerenciamento de usuários e autenticação segura com JWT.",
  "main": "index.js",
  "scripts": {
    "start": "node build/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsup src --out-dir build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/jwt": "9.1.0",
    "@prisma/client": "6.5.0",
    "bcryptjs": "3.0.2",
    "dayjs": "1.11.13",
    "dotenv": "16.4.7",
    "fastify": "5.2.2",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "2.2.2",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "22.13.17",
    "@types/supertest": "6.0.3",
    "@vitest/coverage-v8": "3.1.1",
    "@vitest/ui": "3.1.1",
    "eslint": "8.57.1",
    "npm-run-all": "4.1.5",
    "prisma": "6.5.0",
    "supertest": "7.1.0",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.1"
  }
}
